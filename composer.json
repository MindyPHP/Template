{
    "name": "mindy/template",
    "description": "Fast PHP templating engine",
    "keywords": [
        "template",
        "templating"
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mindy\\Template\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Template\\Tests\\": "./Tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "psr/log": "^1.0",
        "symfony/filesystem": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/var-dumper": "^3.4"
    },
    "authors": [
        {
            "name": "Falaleev Maxim",
            "email": "max@studio107.ru"
        },
        {
            "name": "Nofriandi Ramenta",
            "email": "nramenta@gmail.com"
        }
    ]
}
